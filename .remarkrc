{
  "settings": {
    "bullet": "*",
    "ruleRepetition": 3,
    "fences": true,
    "emitParseErrors": false
  },
  "plugins": [
    "remark-math",
    "remark-preset-lint-recommended",
    "remark-gfm",
    [
      "remark-lint-maximum-line-length",
      [
        "off"
      ]
    ],
    [
      "remark-lint-emphasis-marker",
      [
        "off"
      ]
    ],
    [
      "remark-lint-list-item-indent",
      [
        "off"
      ]
    ],
    [
      "remark-lint-no-undefined-references",
      [
        "off"
      ]
    ]
  ]
}
